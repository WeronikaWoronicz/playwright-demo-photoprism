  "type": "module",
  "scripts": {
    "lint": "pnpm exec eslint src test",
    "lint:fix": "pnpm run lint -- --fix",
    "prettier": "pnpm exec prettier src test --check",
    "prettier:fix": "pnpm run prettier -- --write",
    "format": "pnpm run prettier:fix && pnpm run lint:fix",
    "check-types": "pnpm exec tsc --noEmit",
